{% extends 'base.html.twig' %}

{% block title %}
Edit Removed Country - Digital Beer Wall
{% endblock %}

{% block body %}
<div class="container">
  <div class="card">
    <div class="card-body">
      <div class="row">
        {{ form_start(form) }}
        {{ form_row(form.countryname) }}
        {{ form_row(form.reason) }}
        <br>

        <span class="float-end">
          {{ form_widget(form.save) }}
          {{ form_widget(form.restore) }}
        </span>
        {{ form_end(form) }}
      </div>
    </div>
  </div>
</div>
{% endblock %}